<script>
$.get("nav.html", function(data){
    $("#nav-placeholder").replaceWith(data);
});
</script>

<body>
    
<div id="nav-placeholder"></div>

    <div class="container" style="background-color: rgb(221, 252, 241); padding:2%;">
        <h5>
            Your details are as shown below.
        </h5>
        <br>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;"> Name </span>: {{user.name}}
        </div>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">DOB </span>: {{user.dob}}
        </div>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">Phone </span>: {{user.phone}}
        </div>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">Email </span>: {{user.email}}
        </div>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">Aadhaar No. </span>: {{user.aadhaar}}
        </div>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">Appointment Date </span>: {{user.appt_date}}
        </div>
        <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">Hospital </span>: {{user.hospital_name}}, {{user.hospital_city}}
        </div>
         <div class="usercard">
            <span style="min-width: 15%; display: inline-block;">Result</span>: {{user.result}}
        </div>
    </div>
    <div id="foot-placeholder"></div>
</body>
<script src="//code.jquery.com/jquery.min.js"></script>
